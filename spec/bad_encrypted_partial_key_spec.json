{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "sender_pubkey": {
        "type": "string",
        "pattern": "^[A-f0-9]{96}$"        
      },
      "receiver_signature": {
        "type": "string",
        "pattern": "^[A-f0-9]{192}$"
      },
      "encrypted_data": {
        "type": "string",
        "pattern": "^[A-f0-9]*$"
      },
      "reseiver_base_secrets_commitment_hash": {
        "type": "string",
        "pattern": "^[A-f0-9]{64}$"
      },
      "receiver_pubkey": {
        "type": "string",
        "pattern": "^[A-f0-9]{96}$"
      }
    },
    "required": [
      "sender_pubkey",
      "receiver_signature",
      "encrypted_data",
      "reseiver_base_secrets_commitment_hash",
      "receiver_pubkey"
    ]
  }